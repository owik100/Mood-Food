@model IEnumerable<Mood_Food.Models.Category>
@{
    Layout = null;
}
<ul class="navbar-nav">

    <li class="nav-item">
        @Html.ActionLink("Całe menu", "Products", "Product", null, new { @class = "nav-link" })
    </li>

    @foreach (var item in Model)
    {
        <li class="nav-item">
            @Html.ActionLink(item.Name, "Products", "Product", new { category = item.Name }, new { @class = "nav-link" })
        </li>
    }
</ul>